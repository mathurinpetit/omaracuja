{% extends "::base.html.twig" %}

{% block title %}Omaracuja - Photos{% endblock %}

{% block connexion %}
    {% render url('fos_user_security_login') %}
{% endblock %}

{% block content %}
    <div class="main">
        <h1>Omaracuja - Photos</h1>
        <br/>
        <div class="col-md-12">
            <div class="row">
                {% include 'OmaracujaFrontBundle:Front:picture_buttons_navigation.html.twig' with {'next_month': next_month, 'next_month_label' : next_month_label, 'last_month': last_month , 'last_month_label' : last_month_label } %}

                <br/>

                {% include 'OmaracujaFrontBundle:Front:picture_show.html.twig' with {'picturesByMonth': picturesByMonth } %}

            </div>
        </div>
        {% include 'OmaracujaFrontBundle:Front:picture_view_modal.html.twig' with {'picturesByMonth': picturesByMonth } %}
    </div>  
{% endblock %}