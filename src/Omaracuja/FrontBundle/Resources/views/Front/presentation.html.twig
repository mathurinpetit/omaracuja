{% extends "::base.html.twig" %}

{% block title %}O Maracuj√°{% endblock %}

{% block connexion %}
    {% render url('fos_user_security_login') %}
{% endblock %}

{% block content %}

    {% block video %}
        <div id="video-wrapper"></div>
    {% endblock %}

    <div class="energie"  data-url="{{ path('front_bulle_maracuja') }}" style="cursor: pointer;"></div>



    <div id="mainContainer" class="container">
        <div class="main">         
            <div class="row">
                <div class="col">&nbsp;</div>
            </div>
        </div>
    </div>

    <footer>
        <img src="{{ asset('bundles/omaracujafront/images/nexth.png')}}" id="coming-soon">
        <img src="{{ asset('bundles/omaracujafront/images/nextv.png')}}" id="coming-soon-v">		 
        <ul class="list-evenements"></ul>
    </footer>

    <div id="events_view" data-url="{{ path('front_next_evennements') }}"></div>
    <div id="photos_view" data-url="{{ path('front_photos_views') }}"></div>    
    <div id="videos_view" data-url="{{ path('front_videos_views') }}"></div>
    <div id="flow"></div>

    {% for eventByMonth in events %}
        {% for event in eventByMonth %}
            {{ path('front_evennement',{ 'id': event.id }) }}               
        {% endfor %}
    {% endfor %}
    
   
{% endblock %}